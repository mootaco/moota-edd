{
    "name": "mootaco/moota-edd",
    "description": "Moota Payment Gateway extension for Wordpress' Easy Digital Downloads",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Aprilus Lumbantoruan",
            "email": "i@pilus.me"
        }
    ],
    "minimum-stability": "stable",
    "config": {
        "vendor-dir": "lib/vendor"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/mootaco/php-sdk.git"
        }
    ],
    "require": {
        "moota/moota-sdk": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Moota\\EDD\\": "lib/edd"
        },
        "classmap": ["lib/kint"]
    }
}
